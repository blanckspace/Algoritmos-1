
#include <iostream>
#include <conio.h>
#include <string.h>
#include <math.h>

using namespace std;
#define fil 30
#define col 30

struct telemento
{int mat[fil][col];
};

void cargarMatrizDeMatrices (int mat[][],int n,int m)
{int i,j,k,l;
 for(i=0;i<n;i++)
  {for(j=0;j<n;j++)
   {for(k=0;k<n;k++)
    {for(l=0;l<n;l++)
     {cout<<"Ingrese los elementos a cargar ["<<i<<"]["<<j<<"]["<<k<<"]["<<l<<"]: ";
      cin>>mat[i][j].mat[k][l];
     }
    }
  }
}
 void mostrarMatrizDeMatrices (int mat[][],int n,int m)
{int i,j,k,l;
 for(i=0;i<n;i++)
  {for(j=0;j<n;j++)
   {for(k=0;k<n;k++)
    {for(l=0;l<n;l++)
     {cout<<"mat[i][j].mat[k][l]";
     }
     cout<<endl;
    }
    cout<<endl;
  }
  cout<<endl;
}
