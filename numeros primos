#include <iostream>
#include "stdafx.h"
#include "conio.h"
using namespace std;

void elimNoPrimos (int vec[],int &n,int num)
{int i,j,nnro=0;
 for(i=0;i<n;i++)
  {if(vec[i]==num;
   {for(j=0;j<n;j++)
    {vec[j]=vec[j+1];
    }
  }
}
bool (int n)
{int i;
bool k=true;
 for(i=2;i<n;i++)
  {if(n%i==0)
   {k=false;
   }
  }
return k;
}
