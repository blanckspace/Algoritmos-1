#include <iostream>
#include "stdafx.h"
#include "conio.h"
#define FIL 10
#define COL 10
using namespace std;
void  cargarMatriz (int mat[FIL][COL],int n,int m)
{int i,j;
 for(i=0;i<n;i++)
  {for(j=0;j<m;j++)
   {cout<<"Introducir la matriz: ";
    cin>>mat[i][j];
   }
  }
}
void mostrarMatriz(int mat[FIL][COL],int n,int m)
{int i,j;
 for(i=0;i<n;i++)
  {for(j=0;j<m;j++)
   {cout<<"mat[i][j] ";
   }
  }
}
